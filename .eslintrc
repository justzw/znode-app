{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "extends": ["standard", "airbnb"],
  "rules": {
    "semi": [0],
    "comma-dangle": [0],
    "react/jsx-filename-extension": [0],
    "space-before-function-paren": [0],
    "no-param-reassign": [0]
  }
}
